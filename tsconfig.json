{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@css/*": [
        "src/static/css/*"
      ],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "allowImportingTsExtensions": true
  },
}